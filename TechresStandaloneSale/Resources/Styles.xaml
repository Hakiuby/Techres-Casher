                            <TextBox BorderThickness="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Grid.Column="0" Background="Transparent"  x:Name="contentPresenter" Text="{TemplateBinding Text}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            </TextBox>
                            <Image Grid.Column="1" HorizontalAlignment="Right" Source="/Resources/Images/fast-time.png" Height="20" Width="20" Margin="0 0 10 0" />
                        </Grid>
                    </Border>
                </ControlTemplate>
