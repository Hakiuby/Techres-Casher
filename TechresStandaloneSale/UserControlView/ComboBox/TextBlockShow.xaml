<UserControl x:Class="TechresStandaloneSale.UserControlView.ComboBox.TextBlockShow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TechresStandaloneSale.UserControlView.ComboBox"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="800">
    <Label Height="40" VerticalContentAlignment="Center" Content="{Binding TextContent}" Padding="5 0 5 0" Foreground="Black" FontSize="{StaticResource fontSize}"/>
</UserControl>
