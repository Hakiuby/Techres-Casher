<UserControl x:Class="TechresStandaloneSale.UserControlView.ComboBox.TextboxOther"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:helps="clr-namespace:TechresStandaloneSale.Helpers"
             xmlns:local="clr-namespace:TechresStandaloneSale.UserControlView.ComboBox"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="800">
    <TextBox Background="{StaticResource GrayBackgroundBrush}" MaxLength="50" Padding="3 0 0 0" Text="{Binding TextOther}" Height="40" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="Black" FontSize="{StaticResource fontSize}" BorderBrush="{StaticResource LineListViewBrush}"
                    BorderThickness="0" Name="name" Tag="{Binding Source={x:Static helps:MessageValue.MESSAGE_FROM_TEXT_BOX_OTHER_TAG}}"/>
</UserControl>
