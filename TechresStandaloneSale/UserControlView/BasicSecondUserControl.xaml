<UserControl x:Class="TechresStandaloneSale.UserControlView.BasicSecondUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TechresStandaloneSale.UserControlView"
             mc:Ignorable="d">
    <UserControl.Resources>
        <BitmapImage x:Key="logo_ngang" UriSource="../Resources/Images/banner_main_second.png"/>
    </UserControl.Resources>
    <Grid>
        <Image RenderOptions.BitmapScalingMode="Fant" Stretch="Uniform" Source="{StaticResource logo_ngang}"></Image>
    </Grid>
</UserControl>
